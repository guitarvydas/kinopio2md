KinopioCards {
  SemanticJSON = "{" s<"cards"> ":" "{" Card+ "}" "," s<"connections"> ":" "{" Connection+ "}" "}"
  Card = ID ":" "{" s<"id"> ":" ID "," s<"name"> ":" Text "}" ","?
  Connection = ID ":" "{" s<"id"> ":" ID "," s<"from"> ":" ID "," s<"to"> ":" ID "," s<"type"> ":" ID "}" ","?

  ID = string
  Text = string
  string = dq (~dq any)* dq
  dq = "\""
  s<str> = dq str dq
}
